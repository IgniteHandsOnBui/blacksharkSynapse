{
	"name": "STAC Oblique Pipeline",
	"properties": {
		"description": "Copying data from a source, to a destination, of JPG oblique images while adding STAC components.",
		"activities": [
			{
				"name": "JPG Source to Dest",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFileName": "*.JPG",
							"deleteFilesAfterCompletion": false
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings",
							"copyBehavior": "PreserveHierarchy"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "STAC_Source",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "STAC_Dest",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "STACify_JPG",
				"type": "SynapseNotebook",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebook": {
						"referenceName": "STACify_JPG",
						"type": "NotebookReference"
					},
					"parameters": {
						"JPG_Dest": {
							"value": {
								"value": "@pipeline().parameters.JPG_Destination_Dir",
								"type": "Expression"
							},
							"type": "string"
						}
					},
					"snapshot": true
				}
			},
			{
				"name": "Set variable1",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "STACify_JPG",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"variableName": "TestOut",
					"value": {
						"value": "@activity(STACify_JPG).output.status.Output.result.exitValue",
						"type": "Expression"
					}
				}
			}
		],
		"parameters": {
			"JPG_Destination_Dir": {
				"type": "string",
				"defaultValue": "https://innoaoaidata.blob.core.windows.net/liamdest/RBU_STAC_DEST/"
			}
		},
		"variables": {
			"TestOut": {
				"type": "String"
			}
		},
		"annotations": []
	}
}